tyCheck()\r\n{\r\nreturn new List<Object>() { Value };\r\n}\r\n}"},{"id":"id-70059bdb","name":"id","type":"DataObject","layer":"application","documentation":""},{"id":"e7b676d5","name":"Identify the identity with (object)","type":"ApplicationFunction","layer":"application","documentation":""},{"id":"aea7f763","name":"some function","type":"ApplicationFunction","layer":"application","documentation":""},{"id":"id-9acf22f5","name":"class 1","type":"ApplicationComponent","layer":"application","documentation":""},{"id":"c45f91a8","name":"id","type":"DataObject","layer":"application","documentation":""},{"id":"id-982e917d","name":"equals (object of class 1)","type":"ApplicationFunction","layer":"application","documentation":""},{"id":"d548697f","name":"Compare that identifiers are equal","type":"ApplicationProcess","layer":"application","documentation":""},{"id":"f3b43508","name":"equals (some object)","type":"ApplicationFunction","layer":"application","documentation":""},{"id":"b5d1af68","name":"check that the object of type class 1","type":"ApplicationProcess","layer":"application","documentation":""},{"id":"id-6e46a2f5","name":"attributes","type":"DataObject","layer":"application","documentation":""},{"id":"id-3861a506","name":"equals (object of value type 1)","type":"ApplicationFunction","layer":"application","documentation":""},{"id":"fc0680fc","name":"Compare that attributes are equal","type":"ApplicationProcess","layer":"application","documentation":""},{"id":"f3e80a50","name":"equals (some object)","type":"ApplicationFunction","layer":"application","documentation":""},{"id":"ccad49c8","name":"check that the object of type value type 1","type":"ApplicationProcess","layer":"application","documentation":""},{"id":"cc78c5a5","name":"return new instance of value type 1","type":"ApplicationProcess","layer":"application","documentation":""},{"id":"f83fbb40","name":"value object","type":"ApplicationInterface","layer":"application","documentation":""},{"id":"id-467670ce","